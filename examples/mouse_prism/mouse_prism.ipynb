    "       'train_subjects' : ['G6AE6','G6AE2','G6AE3','G6AE5'],\n",
    "       'test_subjects'  : ['G6AE1'],\n",
    "       'actions'        : ['control']}\n",
    "\n",
    "# merge with training parameters\n",
    "par_data = yaml.full_load(open('param.yaml', \"rb\"))\n",
